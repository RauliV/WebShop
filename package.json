  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "coverage": "nyc --reporter=lcov --reporter=html --reporter=json mocha --require test/setup.test.js --ignore 'test/8.4/**/*.js' --ignore 'test/8.5/**/*.js' --ignore 'test/8.6/**/*.js' --ignore 'test/9.2/**/*.js' --ignore 'test/9.6/**/*.js' --ignore 'test/10.2/**/*.js' --ignore 'test/models/env.test.js'",
