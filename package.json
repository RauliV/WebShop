    "reset-db": "node ./setup/reset-db.js",
    "create-orders": "node ./setup/create-orders.js",
    "eslint": "eslint .",
    "fp-lint": "eslint --no-eslintrc --config ./.fplintrc.json --no-inline-config --ignore-path ./.fplintignore .",
    "jsdoc-lint": "npx eslint --no-eslintrc --config ./.jsdoclintrc.json --no-inline-config --ignore-path ./.jsdoclintignore -o jsdoc_report.html -f html .",
    "test-8.4": "mocha  --timeout 10000 --recursive test/8.4",
    "test-8.5": "mocha --exit --timeout 10000 --recursive test/8.5",
    "test-8.6": "mocha --exit --timeout 10000 --recursive test/8.6",
    "test-9.2": "mocha --exit --timeout 10000 --recursive test/9.2",
    "test-9.4": "mocha --exit --timeout 10000 test/9.6/models/db.test.js test/9.6/models/env.test.js",
    "test-9.5": "mocha --exit --timeout 10000 --recursive test/9.6/models",
    "test-9.6": "mocha --require test/9.6/setup.test.js --exit --timeout 10000 --recursive test/9.6",
    "test-10.2": "mocha --require test/10.2/setup.test.js --exit --timeout 10000 --recursive test/10.2",
    "test": "mocha --require test/setup.test.js --ignore 'test/8.4/**/*.js' --ignore 'test/8.5/**/*.js' --ignore 'test/8.6/**/*.js' --ignore 'test/9.2/**/*.js' --ignore 'test/9.6/**/*.js' --ignore 'test/10.2/**/*.js'"
  },
